# Nonblocking scheduler tests:
#   asynch local:    20
#   message passing: p0 (peer), p1 (ded master)
#   hybrid:          p2 (peer), p3 (ded master)

environment,
	graphics tabular_data
	method_pointer = 'SBUQ'

method,
	id_method = 'SBUQ'
