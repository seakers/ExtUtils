	  variance_based_decomp

model,
	id_model = 'SURROGATE'
	surrogate hierarchical
	  low_fidelity_model  = 'LOFI'
	  high_fidelity_model = 'HIFI'
	  correction additive zeroth_order	#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17,#s18,#s19,#s20,#p0,#p1,#p2,#p3
#	  correction additive first_order	#s10	

variables,
	normal_uncertain = 2			#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17,#s18,#s19,#s20,#p0,#p1,#p2,#p3
      	  means             =  0.   0.		#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17,#s18,#s19,#s20,#p0,#p1,#p2,#p3
	  std_deviations    =  1.   1.		#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17,#s18,#s19,#s20,#p0,#p1,#p2,#p3
#	uniform_uncertain = 2			#s10
#	  lower_bounds      = -5.  -5.		#s10
#	  upper_bounds	    =  5.   5.		#s10
	  descriptors       = 'x1' 'x2'

responses,
	response_functions = 1
	no_gradients	       			#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17,#s18,#s19,#s20,#p0,#p1,#p2,#p3
# 	analytic_gradients			#s10
	no_hessians				

model,
	id_model = 'LOFI'
	single
	  interface_pointer = 'LOFI_FN'

interface,
	id_interface = 'LOFI_FN'
	direct					#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s10,#s11,#s12,#s13,#s14,#s15,#s16,#s17,#s18,#s19
#	fork					#s20,#p0,#p1,#p2,#p3
#	  asynch evaluation_concurrency = 3	#s20,#p2,#p3
 	  analysis_driver = 'lf_rosenbrock'
	  deactivate restart_file

model,
	id_model = 'HIFI'
	single
	  interface_pointer = 'HIFI_FN'

interface,
	id_interface = 'HIFI_FN'
	direct					#s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s10,#s11,#s12,#s13,#s14,#s15,#s16,#s17,#s18,#s19
#	fork					#s20,#p0,#p1,#p2,#p3
#	  asynch evaluation_concurrency = 2	#s20,#p2,#p3
 	  analysis_driver = 'rosenbrock'
	  deactivate restart_file

