#	    piecewise use_derivatives
#	    variance_based_decomp
# Multifidelity test suite
	model_pointer = 'SURROGATE'
	polynomial_chaos				#s0,#s1,#s2,#s6,#s7,#s11,#s12,#s13,#s14,#s15,#s16,#s17,#s18,#s19,#s20,#p0,#p1,#p2,#p3
#	  expansion_order = 2 4				#s11,#s13,#s15,#s16,#s18
