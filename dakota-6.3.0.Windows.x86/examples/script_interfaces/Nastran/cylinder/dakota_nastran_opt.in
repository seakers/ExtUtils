environment,
	graphics

method,
	dot_sqp

variables,
	continuous_design = 1
	  initial_point = .2
	  lower_bounds = .05
	  upper_bounds = 100.
	  descriptor = 'shell_thick'

interface,
	 system asynch evaluation_concurrency = 4
	  analysis_driver = 'nastran_script'
	  parameters_file = 'params.in'
	  results_file = 'results.out'
	  file_tag file_save aprepro

responses,
	objective_functions = 1
	nonlinear_inequality_constraints = 1
	nonlinear_inequality_lower_bounds = 3000.
	nonlinear_inequality_upper_bounds = 10000.
	numerical_gradients
	  method_source dakota
	  interval_type central
	  fd_grad_step_size = .005
	no_hessians
