# Dakota Input File: rosen_ps_vector.in

environment
  graphics
  tabular_data
    tabular_data_file = 'rosen_ps_vector.dat'

method
  vector_parameter_study
    final_point = 1.1  1.3
    num_steps = 10

model
  single

variables
  continuous_design = 2
    initial_point    -0.3      0.2
    descriptors       'x1'     "x2"

interface
  analysis_driver = 'rosenbrock'
    direct

responses
  objective_functions = 1
  no_gradients
  no_hessians
